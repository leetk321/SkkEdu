<query id="deleteGroup" action="delete">
    <tables>
        <table name="member_group_member" />
    </tables>
	<conditions>
		<condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" />
		<condition operation="equal" column="group_srl" var="present_group_srl" pipe="and" />
		<condition operation="equal" column="site_srl" var="site_srl" default="0" pipe="and" />
	</conditions>
</query>

