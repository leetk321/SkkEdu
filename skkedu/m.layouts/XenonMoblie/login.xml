<?xml version="1.0" encoding="utf-8"?>
<filter name="login" module="member" act="procMemberLogIn">
	<form>
		<node target="user_id" required="true" filter="user_id" />
		<node target="password" required="true" />
	</form>
	<parameter />
	<response callback_func="completeLogIn">
		<tag name="error" />
		<tag name="message" />
	</response>
</filter>